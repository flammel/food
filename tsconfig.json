{
    "compilerOptions": {
        "noEmit": true,
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "lib": ["es2017", "dom", "dom.iterable"],
        "allowSyntheticDefaultImports": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "downlevelIteration": true
    }
}
